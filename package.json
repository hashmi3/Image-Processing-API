  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hashmi3/Image-Processing-API/issues"
  },
  "homepage": "https://github.com/hashmi3/Image-Processing-API#readme",
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/jasmine": "^3.10.3",
    "@types/node": "^17.0.10",
    "@types/sharp": "^0.29.5",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "@typescript-eslint/parser": "^5.10.0",
    "eslint": "^8.7.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "fs": "^0.0.1-security",
    "jasmine": "^4.0.2",
    "jasmine-spec-reporter": "^7.0.0",
    "prettier": "^2.5.1",
    "sharp": "^0.29.3",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "express": "^4.17.2",
    "supertest": "^6.2.2"
  }
