    "installJasmine": "npm i jasmine -D && npm i jasmine-spec-reporter -D && npm  i @types/jasmine -D",
    "prettier": "prettier --config .prettierrc 'src/**/*.ts' --write",
    "lint": "eslint ./src/**/*.ts",
    "jasmine": "jasmine",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hashmi3/Image-Processing-API.git"
  },
